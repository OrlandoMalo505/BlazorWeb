<div class="row">
    <h4 class="text-primary p-lg-4 pt-2 col-12">Grand Child Component</h4>
    <p>There is a message - @MessageForGrandChild</p><br />
    <p>Lucky Number - @LuckyNumber</p>

</div>
@code {
    [CascadingParameter(Name ="message")]
    public string MessageForGrandChild { get; set; }

    [CascadingParameter(Name ="lucky")]
    public string LuckyNumber { get; set; }
}
